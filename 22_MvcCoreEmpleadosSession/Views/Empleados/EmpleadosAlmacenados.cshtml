

<h1>Empleados almacenados</h1>

@{
    List<Empleado> empleados = Context.Session.GetObject<List<Empleado>>("EMPLEADOS");
}

<a asp-controller="Empleados" asp-action="SessionEmpleados">Volver</a>

@if(empleados == null)
{
    <h1 style="color:red">No ha empleados en session</h1>
} else
{
    <ul class="list-group">
        @foreach (Empleado emp in empleados)
        {
            <li class="list-group-item list-group-item-info">
                Apellido: @emp.Apellido, Oficio: @emp.Oficio, Salario: @emp.Salario
            </li>
        }
    </ul>
}


