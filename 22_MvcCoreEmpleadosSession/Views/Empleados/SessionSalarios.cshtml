@model List<Empleado>

<h1>Session Salarios</h1>

<a asp-controller="Empleados" asp-action="SumaSalarios">Suma salarial salarios</a>

<h1 style="color:red">@ViewData["MENSAJE"]</h1>

<ul class="list-group">
    @foreach(Empleado emp in Model)
    {
        <li class="list-group-item">
            @emp.Apellido
            <a asp-controller="Empleados" asp-action="SessionSalarios" asp-route-salario="@emp.Salario">Almacenar Salarios</a>
        </li>
    }
</ul>

