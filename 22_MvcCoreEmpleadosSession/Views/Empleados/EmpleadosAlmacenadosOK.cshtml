@model List<Empleado>


<h1>Empleados Almacenados Session OK</h1>


@if(Context.Session.GetObject<List<int>>("IDSEMPLEADOS") == null)
{
    @*<h1 style="color:red">@ViewData["MENSAJE"]</h1>*@
    <h1 style="color:red">No existen empleados en session</h1>
}

@if (Model != null)
{
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Apellido</th>
                <th>Oficio</th>
                <th>Salario</th>
            </tr>
        </thead>
        <tbody>
            @foreach(Empleado emp in Model)
            {
                <tr>
                    <td>@emp.Apellido</td>
                    <td>@emp.Oficio</td>
                    <td>@emp.Salario</td>
                    <td>
                        <a asp-controller="Empleados" asp-action="EmpleadosAlmacenadosOK" asp-route-idEliminar="@emp.IdEmpleado">Quitar de Session</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
}